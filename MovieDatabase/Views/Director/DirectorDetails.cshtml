@model DirectorViewModel

<div class="container">
    <ul class="nav marginbottom25">
        <h2>@Model.Name, @Model.DateOfBirth.ToString("d")</h2>
        <br />
        <h4 class="underline">Movies directed</h4>
        @foreach (var movie in Model.DirectedMovies)
        {
            <li>@movie.Title, @movie.ProductionYear</li>
        }
    </ul>
</div>